	<item>
		<title>メインスタジオ</title>
		<link>http://localhost/studio/main-studio/</link>
		<pubDate>Mon, 11 Nov 2025 10:00:00 +0000</pubDate>
		<dc:creator><![CDATA[admin]]></dc:creator>
